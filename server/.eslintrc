{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": { "ecmaVersion": "latest" },
  "rules": {
    "no-unsafe-optional-chaining": "off",
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-console": "warn",
    "indent": ["warn", 2],
    "quotes": ["warn", "double"],
    "prefer-const": "warn",
    "no-const-assign": "warn"
  },
  "plugins": ["prettier"]
}
